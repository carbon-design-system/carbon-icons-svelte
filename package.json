    "svelte": "^5.43.14",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbon-design-system/carbon-icons-svelte.git"
  },
  "homepage": "https://carbon-icons-svelte.onrender.com/",
  "bugs": "https://github.com/carbon-design-system/carbon-icons-svelte/issues",
  "keywords": [
